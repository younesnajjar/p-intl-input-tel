{
  "name": "p-intl-input-tel",
  "version": "17.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --coverage",
    "test-ci": "jest --runInBand",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/p-intl-input-tel-app/stats.json",
    "build_lib": "ng build p-intl-input-tel",
    "copy-license": "cp ./LICENSE ./dist/p-intl-input-tel",
    "copy-readme": "cp ./README.md ./dist/p-intl-input-tel",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "npm run copy-files && cd dist/p-intl-input-tel && npm pack",
    "create-link": "cd dist/p-intl-input-tel && npm link",
    "package": "npm run build_lib && npm run npm_pack",
    "postinstall": "ngcc"
  },
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@primeng/themes": "^18.0.0",
    "@typescript-eslint/types": "^8.0.0",
    "core-js": "^3.32.0",
    "google-libphonenumber": "^3.2.33",
    "primeflex": "^3.3.1",
    "primeng": "^18.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular-eslint/builder": "18.3.0",
    "@angular-eslint/eslint-plugin": "^18.3.0",
    "@angular-eslint/eslint-plugin-template": "18.3.0",
    "@angular-eslint/schematics": "^18.3.0",
    "@angular-eslint/template-parser": "18.3.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler-cli": "^18.2.0",
    "@angular/language-service": "^18.2.0",
    "@types/google-libphonenumber": "^7.4.24",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.2",
    "ng-packagr": "^18.2.0",
    "prettier": "^3.0.1",
    "ts-node": "~10.9.1",
    "typescript": "^5.4.5",
    "webpack-bundle-analyzer": "^4.9.0"
  }
